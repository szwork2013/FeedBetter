cleos set contract feedbetter ./ feedbetter.wast feedbetter.abi


cleos push action feedbetter init '{"issuer": "feedbetter"}' -p feedbetter@active

cleos push action feedbetter send '{"from": "feedbetter", "to": "lecko", "quantity": "1000.0000 FBC", "memo": "trans"}' -p feedbetter@active



echo "feedbetter have"
cleos get currency balance feedbetter feedbetter
echo "lecko have"
cleos get currency balance feedbetter lecko
echo "table: accounts"
cleos get table feedbetter feedbetter accounts
cleos get table feedbetter lecko accounts
echo "table: stats"
cleos get table feedbetter FBC stats